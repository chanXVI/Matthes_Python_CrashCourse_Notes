# Intergers, Floars, Underscores, Multiple Assignment
print(2+3)
print(3-2)
print(2*3)
print(10/5)
print(10%5)

print(3**3)
print(10**9)

print(2 +(3*4))
million = 1_000_000
print(million)

x, y, z = 4, 12, 15
print(x)
print(x+z)
#END

